  inherited Query: TADOQuery
    CursorType = ctStatic
    SQL.Strings = (
      ''
      'Select '
      #9'Recno, Empresa, ID, Item, Tipo, Semana '
      'from Pcp_TB_ProdutoGrupoItem Grupo'
      'Where Empresa = '#39'0101'#39
      'AND Tipo is not Null'
      'Order by Recno desc'
      '')
