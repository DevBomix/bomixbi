  inherited Query: TADOQuery
    SQL.Strings = (
      'Select '
      ''
      
        '    Recno, Area, Setor, OrdemProducao, Cliente, SetorMercado, St' +
        'atus, Emissao,'
      
        ' '#9'SUBSTRING(Produto_ID,1,4) as Item, Produto_ID, Produto, DataAj' +
        'ustadaIni, HoraAjustadaIni, Prioridade, '
      ' '#9'CASE  '
      
        ' '#9#9#9'WHEN DATEPART(WEEK, DataAjustadaIni) <= 9 THEN '#39'0'#39' + CAST(DA' +
        'TEPART(WEEK, DataAjustadaIni) AS varchar(1))  '
      
        ' '#9'ELSE CAST(DATEPART(WEEK, DataAjustadaIni) AS varchar(2)) END +' +
        ' SUBSTRING(Produto_ID,1,4) + CAST(YEAR(DataAjustadaIni) AS varch' +
        'ar(4)) AS Lote,'#9
      
        #9'Injetora_FK, CicloPadrao, Molde_ID, Molde, Resina1_ID, Resina1,' +
        ' Resina1Perc, Resina2_ID, Resina2, Resina2Perc, Master_ID, Maste' +
        'r, MasterPerc'
      ''
      'from BOMIXBI.dbo.Pcp_TB_OrdemProducao (nolock) '
      
        'Where DataAjustadaIni >= CONVERT(Datetime,Convert(Varchar(10),'#39'0' +
        '2/03/2020'#39',103),103)'
      
        'AND DataAjustadaIni <= CONVERT(Datetime,Convert(Varchar(10),'#39'08/' +
        '03/2020'#39',103),103)'
      
        'AND Setor + OrdemProducao + Cliente + SetorMercado + Injetora_FK' +
        ' + Produto_ID + Produto like '#39'%%'#39
      'AND Injetora_FK = '#39'550-12'#39
      'AND SUBSTRING(Produto_ID,1,4) = '#39'B15B'#39
      'Order by DataAjustadaIni, HoraAjustadaIni, Prioridade')
    Left = 348
    Top = 232
