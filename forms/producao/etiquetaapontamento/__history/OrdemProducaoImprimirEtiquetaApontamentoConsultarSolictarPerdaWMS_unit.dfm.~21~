  inherited Query: TADOQuery
    SQL.Strings = (
      'Select '
      #9'OP.Recno, OP.Empresa, Setor, Area, OrdemProducao, Status, '
      #9'OP.Cliente,'
      #9'Emissao, Injetora as Recurso,'
      #9'OP.Produto_ID, OP.Produto, '
      
        #9'Quantidade, Produzida, Saldo, Produto.Liso_ID as Perda_ID, Liso' +
        '.Produto as Perda'
      'from BomixBI.dbo.Pcp_TB_OrdemProducao OP (nolock)'
      
        'Inner Join BomixBI.dbo.Pcp_TB_Produto Produto (nolock) ON Produt' +
        'o.Empresa = '#39'0101'#39
      #9#9#9#9#9#9#9#9#9#9#9#9#9'  AND Produto.Produto_ID = OP.Produto_ID'
      
        'Inner Join BomixBI.dbo.Pcp_TB_Produto Liso (nolock) ON Liso.Empr' +
        'esa = '#39'0101'#39
      #9#9#9#9#9#9#9#9#9#9#9'       AND Produto.Liso_ID = Liso.Produto_ID'
      ''
      'Where Area = '#39'PERSONALIZACAO'#39
      'AND OP.Empresa = '#39'0101'#39
      'AND Prioridade < 500'
      'AND Status not in ('#39'FINALIZADA'#39','#39'SUSPENSA'#39')'
      'AND Produzida > 0'
      'Order by OP.Recno desc')
