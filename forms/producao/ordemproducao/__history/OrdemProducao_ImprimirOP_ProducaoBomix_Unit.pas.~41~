uses conexaodados_unit, funcaosistema_unit;


procedure TOrdemProducao_ImprimirOP_ProducaoBomix.ColocarMoldeEmOrdemProducaoQueEstavaSem(VLN_Recno : Integer);
Var
   VLC_Update : String;

begin

   VLC_Update := ' Update P12OFICIAL.dbo.SC2010 ' + #13;
   VLC_Update := VLC_Update + ' Set C2_FSMOLID = Molde_ID,' + #13;
   VLC_Update := VLC_Update + ' C2_FSMOLDE = Molde ' + #13;
   VLC_Update := VLC_Update + ' from BomixBI.[dbo].[Icq_TB_Molde_Programacao] (nolock) ' + #13;
   VLC_Update := VLC_Update + ' inner Join P12OFICIAL.dbo.SC2010 C2 (nolock) ON C2_FILIAL = ' + '''' + Sys_FuncaoSistema.PNL_EmpresaTotvs.Caption + '''' + #13;
   VLC_Update := VLC_Update + ' 										 			AND C2.D_E_L_E_T_ <> ' + '''' + '*' + '''' + #13;
   VLC_Update := VLC_Update + ' 													AND C2_RECURSO = Recurso ' + #13;
   VLC_Update := VLC_Update + ' Where R_E_C_N_O_ = ' + IntToStr(VLN_Recno);

   With Sys_ModuloConexaoDados.ADOCommand1 do begin  CommandText := VLC_Update; Execute;  end;

   VLC_Update := ' Update BOMIXBI.dbo.Pcp_TB_OrdemProducao ' + #13;
   VLC_Update := VLC_Update + '		Set Molde_ID = Pro.Molde_ID, ' + #13;
   VLC_Update := VLC_Update + '		Molde = Pro.Molde ' + #13;
   VLC_Update := VLC_Update + '		from BomixBI.[dbo].[Icq_TB_Molde_Programacao] Pro (nolock)  ' + #13;
   VLC_Update := VLC_Update + '		inner Join BOMIXBI.dbo.Pcp_TB_OrdemProducao OP (nolock) ON Empresa = ' + '''' + Copy(Sys_FuncaoSistema.PNL_EmpresaTotvs.Caption,1,4) + '''' + #13;
   VLC_Update := VLC_Update + '													AND Injetora_FK = Recurso  ' + #13;
   VLC_Update := VLC_Update + ' Where Recno = ' + IntToStr(VLN_Recno);

   With Sys_ModuloConexaoDados.ADOCommand1 do begin CommandText := VLC_Update; Execute;  end;


end;

function TOrdemProducao_ImprimirOP_ProducaoBomix.GetRecursoPrioridade(VLC_Recurso: String): String;
Var
   Query : TADOQuery;
   VLC_Select : String;

begin

  VLC_Select := VLC_Select + ' Select H1_FSPRIO from P12OFICIAL.dbo.SH1010 (nolock) ' + #13;
  VLC_Select := VLC_Select + ' Where H1_CODIGO = ' + '''' + VLC_Recurso + '''' + #13;

  Query := TADOQuery.Create(self);
  With Query do
  begin
      Close;
      Connection := Sys_ModuloConexaoDados.ADOConnection1;
      SQL.Clear;
      SQL.Text := VLC_Select;
      Open;
  end;

  if Query.FieldByName('H1_FSPRIO').AsString = 'S' then
      Result := 'Sim'
  else
      Result := 'Não';

  Query.Free;

end;


procedure TOrdemProducao_ImprimirOP_ProducaoBomix.ImprimirEmpenho(
  VLC_Ordem: String);
Var
   VLC_Select : String;
begin


    VLC_Select := VLC_Select + ' Select ' + #13;
    VLC_Select := VLC_Select + ' ' + #13;
    VLC_Select := VLC_Select + ' 	D4.R_E_C_N_O_ as Recno, Rtrim(D4_OP) as OrdemProducao, ' + #13;
    VLC_Select := VLC_Select + ' 	Rtrim(D4_COD) as Produto_ID,  Rtrim(D4_FSDSC) as Produto, ' + #13;
    VLC_Select := VLC_Select + ' 	Rtrim(D4_LOCAL) as Armazem,	D4_QUANT as Empenhado, ' + #13;
    VLC_Select := VLC_Select + ' 	D4_FSPM_R as PercMasterResina, Rtrim(B1_UM) as Unidade ' + #13;
    VLC_Select := VLC_Select + ' ' + #13;
    VLC_Select := VLC_Select + ' from P12OFICIAL.dbo.SD4010 D4 (nolock) ' + #13;
    VLC_Select := VLC_Select + ' Inner Join P12OFICIAL.dbo.SB1010 B1 (nolock) ON B1_FILIAL = ' + '''' + Copy(Sys_FuncaoSistema.PNL_EmpresaTotvs.Caption,1,4) + '''' + #13;
    VLC_Select := VLC_Select + ' 											AND B1.D_E_L_E_T_ <> ' + '''' + '*' + '''' + #13;
    VLC_Select := VLC_Select + ' 											AND B1_COD = D4_COD ' + #13;
    VLC_Select := VLC_Select + ' where D4_FILIAL = ' + '''' + Sys_FuncaoSistema.PNL_EmpresaTotvs.Caption + '''' + #13;
    VLC_Select := VLC_Select + ' AND D4.D_E_L_E_T_ <> ' + '''' + '* ' + '''' + #13;
    VLC_Select := VLC_Select + ' AND D4_OP = ' + '''' + VLC_Ordem + '''' + #13;
    VLC_Select := VLC_Select + ' Order by PercMasterResina desc ' + #13;

    CDS_ProdutoEmpenhado.Close;
    Query_ProdutoEmpenhado.Close;
    Query_ProdutoEmpenhado.SQL.Clear;
    Query_ProdutoEmpenhado.SQL.Text := VLC_Select;
    CDS_ProdutoEmpenhado.Open;

end;

procedure TOrdemProducao_ImprimirOP_ProducaoBomix.ImprimirOrdemBomixProducao(VLC_Ordem : String; VLN_Recno: Integer);
Var
   VLC_Select : String;
   VLC_Update : String;

begin

       ImprimirEmpenho(VLC_Ordem);

       VLC_Select := ' Select ' + #13;
       VLC_Select := VLC_Select + ' Cast(Carrinho as Varchar(10)) + ' + '''' + ' / ' + '''' + ' + Cast(QtdCarrinho as Varchar(10)) as DadosCarrinho,' + #13;
       VLC_Select := VLC_Select + ' * from BomixBI.dbo.Pcp_TB_OrdemProducao (Nolock) ' + #13;
       VLC_Select := VLC_Select + ' Where OrdemProducao = ' + '''' + VLC_Ordem + '''' + #13;
       VLC_Select := VLC_Select + ' AND Empresa = ' + '''' + Copy(Sys_FuncaoSistema.PNL_EmpresaTotvs.Caption,1,4) + '''' + #13;
       if GetRecursoPrioridade(VLC_Ordem) = 'Sim' then
             VLC_Select :=  VLC_Select + ' AND Prioridade < 500' + #13;

       CDS_ImpressaoOP.Close;
       Query_ImpressaoOP.Close;
       Query_ImpressaoOP.SQL.Clear;
       Query_ImpressaoOP.SQL.Text := VLC_Select;
       CDS_ImpressaoOP.Open;

       if CDS_ImpressaoOPMolde_ID.AsString = '' then
       begin
            ColocarMoldeEmOrdemProducaoQueEstavaSem(CDS_ImpressaoOPRecno.AsInteger);
       end;


       ppLBL_Usuario.Caption := 'Data Impressão: ' + DateTimeToStr(Date()) + ' - ' +  TimeToStr(Time());
       ppLBL_Data.Caption := Sys_FuncaoSistema.PNL_UsuarioAtivo.Caption;

       ppImage4.Picture := Sys_FuncaoSistema.Img_LogoBomix.Picture;

       If CDS_ImpressaoOPItemBasf.AsString = 'Sim' then ppItensBasf.Visible := True else ppItensBasf.Visible := False;

       ppImprimirInjecao.Print;

end;

procedure TOrdemProducao_ImprimirOP_ProducaoBomix.ppLabel_ZerarRotuloPrint(
  Sender: TObject);
begin
    if CDS_ImpressaoOPZerarRotulo.AsString = 'Sim' then
        ppLabel_ZerarRotulo.Visible := True
    else
        ppLabel_ZerarRotulo.Visible := False;
end;

end.
