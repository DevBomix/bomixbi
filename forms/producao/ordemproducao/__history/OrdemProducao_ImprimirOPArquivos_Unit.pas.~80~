uses FuncaoSistema_Unit, OrdemProducaoImprimirOP_Unit, ConexaoDados_Unit;

procedure TOrdemProducao_ImprimirOP_Arquivos.ImprimirOrdemBomixPaletizacao(VLC_Ordem : String);
Var
   VLC_Select : String;

begin

       VLC_Select := ' Select * from BomixBI.dbo.Pcp_TB_OrdemProducao (Nolock) ' + #13;
       VLC_Select := VLC_Select + ' Where OrdemProducao = ' + '''' + VLC_Ordem + '''' + #13;

       if OrdemProducao_ImprimirOP.GetRecursoPrioridade(VLC_Ordem) = 'Sim' then
             VLC_Select :=  VLC_Select + ' AND Prioridade < 500' + #13;

       VLC_Select := VLC_Select + ' AND Empresa = ' + '''' + Copy(Sys_FuncaoSistema.PNL_EmpresaTotvs.Caption,1,4) + '''' + #13;

       CDS_ImpressaoPaletizacao.Close;
       Query_ImpressaoPaletizacao.Close;
       Query_ImpressaoPaletizacao.SQL.Clear;
       Query_ImpressaoPaletizacao.SQL.Text := VLC_Select;
       CDS_ImpressaoPaletizacao.Open;

       ppLabel547.Caption := 'Data Impresso: ' + DateTimeToStr(Date());
       ppLabel548.Caption := Sys_FuncaoSistema.PNL_UsuarioAtivo.Caption;

       ppImage2.Picture := Sys_FuncaoSistema.Img_LogoBomix.Picture;
       ppImprimirPaletizacao.Print;

end;



procedure TOrdemProducao_ImprimirOP_Arquivos.ImprimirOrdemBomixPersonalizacao(VLC_Ordem : String; VLN_Recno: Integer);
Var
   VLC_Select : String;

begin

       VLC_Select := '  Select Distinct ' + #13;
       VLC_Select := VLC_Select + '  ' + #13;
       VLC_Select := VLC_Select + '   DadosCarrinho, Setor, OrdemProducao, Sequencia, Status, Emissao, DataPCP, DataEncerramento, Tipo, TB.TipoProduto, ' + #13;
       VLC_Select := VLC_Select + '   TB.Produto_ID, TB.Produto, TB.ClasseC, TB.ClasseD, TB.Arte_ID, TB.Arte, TB.StatusArte, TB.Personalizacao, ' + #13;
       VLC_Select := VLC_Select + '   Case When ((Produto.PalleteComBalde = ' + '''' + 'Sim' + '''' +  ') Or (TB.PalleteComBalde = ' + '''' + 'Sim' + '''' + ')) ' + #13;
       VLC_Select := VLC_Select + '   then ' + '''' + 'OBS: PALETIZAO UNIFICADA: BALDE + TAMPA' + '''' + ' else ' + '''' + '' + '''' + ' end PalleteComBalde, ' + #13;
       VLC_Select := VLC_Select + '   Produto.Produto_ID as Tamparia_ID,  Produto.Produto + ' + '''' + ' <---> SALDO (E2): ' + '''' + #13;
       VLC_Select := VLC_Select + '   + (Select Top 1 Cast(B2_QATU as Varchar(10)) from P12OFICIAL.dbo.SB2010 (nolock) where B2_LOCAL = ' + '''' + 'E2' + '''' + #13;
       VLC_Select := VLC_Select + '   AND B2_FILIAL = ' + '''' + '010101' + '''' + ' AND B2_COD = Produto.Produto_ID) as Tamparia, Roteiro, Injetora_FK, Injetora, ' + #13;
       VLC_Select := VLC_Select + '   Ferramenta, TB.Item, Quantidade, Produzida, Saldo, TB.CentroCusto, Pedido, ' + #13;
       VLC_Select := VLC_Select + '   QtdPorEmbalagem, TB.Grupo, TB.Peso,	VolumeNominal, GrupoEmp, Empenhado_ID, ProdutoEmp, ' + #13;
       VLC_Select := VLC_Select + '   ArmazemEmp, UndEmp, Empenhado  ' + #13;
       VLC_Select := VLC_Select + ' ' + #13;
       VLC_Select := VLC_Select + '  from ( ' + #13;
       VLC_Select := VLC_Select + ' ' + #13;
       VLC_Select := VLC_Select + '     Select Distinct ' + #13;
       VLC_Select := VLC_Select + '        Cast(Carrinho as Varchar(10)) + ' + '''' + ' / ' + '''' + ' + Cast(QtdCarrinho as Varchar(10)) as DadosCarrinho, ' + #13;
       VLC_Select := VLC_Select + '        Item.Mix, Case When Substring(Item.Produto,1,1) = ' + '''' + 'B' + '''' + ' then ' + #13;
       VLC_Select := VLC_Select + '        (Select top 1 Produto_ID from BomixBI.dbo.Fat_TB_PedidoVendaItem  Tampa Where Item.Mix = Tampa.Mix ' + #13;
       VLC_Select := VLC_Select + '        AND Tampa.Pedido_FK = Item.Pedido_FK AND Tampa.C6_Empresa = Item.C6_Empresa AND TipoProduto = ' + '''' + 'TAMPA' + '''' +  ')' + #13;
       VLC_Select := VLC_Select + '        else ' + #13;
       VLC_Select := VLC_Select + '        (Select top 1 Produto_ID from BomixBI.dbo.Fat_TB_PedidoVendaItem  Balde Where Item.Mix = Balde.Mix' + #13;
       VLC_Select := VLC_Select + '        AND Balde.Pedido_FK = OrdemProducao.Pedido AND Balde.C6_Empresa = OrdemProducao.Empresa AND Balde.TipoProduto = ' + '''' + 'BALDE' + '''' +  ')' + #13;
       VLC_Select := VLC_Select + '        end as Tamparia_ID, OrdemProducao.*, Empenhado.Grupo as GrupoEmp,' + #13;
       VLC_Select := VLC_Select + '        Empenhado.Produto_ID as Empenhado_ID, Empenhado.Produto as ProdutoEmp,' + #13;
       VLC_Select := VLC_Select + '        Empenhado.Unidade as UndEmp, Empenhado, Empenhado.Armazem as ArmazemEmp' + #13;
       VLC_Select := VLC_Select + ' ' + #13;
       VLC_Select := VLC_Select + '   from BomixBI.dbo.Pcp_TB_OrdemProducao (Nolock) OrdemProducao' + #13;
       VLC_Select := VLC_Select + '   Left Join BomixBI.dbo.Fat_TB_PedidoVendaItem Item  (nolock) ON Item.Pedido_FK = OrdemProducao.Pedido' + #13;
       VLC_Select := VLC_Select + '   AND Item.Sequencia = OrdemProducao.Sequencia AND Item.C6_Empresa = OrdemProducao.Empresa' + #13;
       VLC_Select := VLC_Select + '   Inner Join BomixBI.dbo.Pcp_TB_ProdutoEmpenho (nolock) Empenhado ON Empenhado.C2_Recno = OrdemProducao.Recno' + #13;
       VLC_Select := VLC_Select + ' Where OrdemProducao.Setor in (' + '''' + 'PERSONALIZACAO' + '''' + ',' + '''' + 'PALETIZACAO PE' + '''' + ')' + #13;

       if OrdemProducao_ImprimirOP.GetRecursoPrioridade(VLC_Ordem) = 'Sim' then
             VLC_Select :=  VLC_Select + ' AND Prioridade < 500' + #13;

       VLC_Select := VLC_Select + ' AND OrdemProducao.OrdemProducao = ' + '''' + VLC_Ordem + '''' + #13;
       VLC_Select := VLC_Select + '                  ) TB' + #13;
       VLC_Select := VLC_Select + '                  Left Join BomixBI.dbo.Pcp_TB_Produto (nolock) Produto ON Produto.Produto_ID = Tamparia_ID' + #13;


       CDS_ImpressaoPersonalizacaoOP.Close;
       Query_ImpressaoPersonalizacaoOP.Close;
       Query_ImpressaoPersonalizacaoOP.SQL.Clear;
       Query_ImpressaoPersonalizacaoOP.SQL.Text := VLC_Select;
       CDS_ImpressaoPersonalizacaoOP.Open;


       if (Copy(CDS_ImpressaoPersonalizacaoOPProduto_ID.AsString,1,4) = 'P02D')
       then
       begin

             ppLabel719.Caption := 'Data Impresso: ' + DateTimeToStr(Date());
             ppLabel720.Caption := Sys_FuncaoSistema.PNL_UsuarioAtivo.Caption;

             if CDS_ImpressaoPersonalizacaoOPPersonalizacao.AsString <> '' then
             begin
                  ppLabel710.Visible := True;
                  ppDBText73.Visible := True;
                  ppLabel709.Visible := True;
                  ppDBText74.Visible := True;
                  ppDBText75.Visible := True;
             end
             else
             begin
                  ppLabel710.Visible := False;
                  ppDBText73.Visible := False;
                  ppLabel709.Visible := False;
                  ppDBText74.Visible := False;
                  ppDBText75.Visible := False;
             end;

             ppImage3.Picture := Sys_FuncaoSistema.Img_LogoBomix.Picture;
             ppImprimirPersonalizacaoHTD.Print;

       end
       else
       begin

             ppLBL_UsuarioPersonalizacao.Caption := 'Data Impresso: ' + DateTimeToStr(Date());
             ppLBL_DataPersonalizacao.Caption := Sys_FuncaoSistema.PNL_UsuarioAtivo.Caption;

             if CDS_ImpressaoPersonalizacaoOPPersonalizacao.AsString <> '' then
             begin
                  ppLabel102.Visible := True;
                  ppDBText19.Visible := True;
                  ppLabel101.Visible := True;
                  ppDBText20.Visible := True;
                  ppDBText37.Visible := True;
             end
             else
             begin
                  ppLabel102.Visible := False;
                  ppDBText19.Visible := False;
                  ppLabel101.Visible := False;
                  ppDBText20.Visible := False;
                  ppDBText37.Visible := False;
             end;

             ppImage1.Picture := Sys_FuncaoSistema.Img_LogoBomix.Picture;
             ppImage8.Picture := Sys_FuncaoSistema.Img_LogoBomix.Picture;
             ppImprimirPersonalizacao.Print;
       end;

end;


procedure TOrdemProducao_ImprimirOP_Arquivos.ImprimirOrdemBomixTinturaria(
  VLC_Ordem: String);
Var
   VLC_Select : String;
begin

   VLC_Select := ' Select  Distinct ' + #13;
   VLC_Select := VLC_Select + ' 		OrdemProducao.*, Empenhado.Grupo as GrupoEmp, ' + #13;
   VLC_Select := VLC_Select + '        Empenhado.Produto_ID as Empenhado_ID, Empenhado.Produto as ProdutoEmp, ' + #13;
   VLC_Select := VLC_Select + '        Empenhado.Unidade as UndEmp, Empenhado, Empenhado.Armazem as ArmazemEmp ' + #13;
   VLC_Select := VLC_Select + ' ' + #13;
   VLC_Select := VLC_Select + ' from BomixBI.dbo.Pcp_TB_OrdemProducao (Nolock) OrdemProducao ' + #13;
   VLC_Select := VLC_Select + ' Inner Join BomixBI.dbo.Pcp_TB_ProdutoEmpenho (nolock) Empenhado ON Empenhado.C2_Recno = OrdemProducao.Recno ' + #13;
   VLC_Select := VLC_Select + ' ' + #13;
   VLC_Select := VLC_Select + ' Where OrdemProducao.Setor = ' + '''' + 'TINTURARIA' + '''';
   VLC_Select := VLC_Select + ' AND OrdemProducao.OrdemProducao = ' + '''' + VLC_Ordem + '''';

   CDS_ImpressaoTinturaria.Close;
   Query_ImpressaoTinturaria.Close;
   Query_ImpressaoTinturaria.SQL.Clear;
   Query_ImpressaoTinturaria.SQL.Text := VLC_Select;
   CDS_ImpressaoTinturaria.Open;

   ppLabel375.Caption := 'Data Impresso: ' + DateTimeToStr(Date());
   ppLabel374.Caption := Sys_FuncaoSistema.PNL_UsuarioAtivo.Caption;

   ppImage7.Picture := Sys_FuncaoSistema.Img_LogoBomix.Picture;
   ppImprimirTinturaria.Print;

end;


end.
