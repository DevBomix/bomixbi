  inherited Query: TADOQuery
    SQL.Strings = (
      'Select '
      #9'* '
      'from ('
      #9#9'Select '
      #9#9#9'H6.R_E_C_N_O_ as Recno, '
      
        #9#9#9'Case when H6_TIPO = '#39'I'#39' then '#39'PARADA'#39' else '#39'PRODUTIVA'#39' end Ti' +
        'po, '
      
        #9#9#9'H6_FSSETOR as Setor, H6_RECURSO as Recurso, Convert(Datetime,' +
        'H6_FSDTTU,112) as DataTurno, H6_FSTURNO as Turno,'
      #9#9#9'H6.H6_HORAINI as HoraInicial, H6.H6_HORAFIN as HoraFim,  '
      
        #9#9#9'H6_OP as OrdemProducao, H6_PRODUTO as Produto_ID, Rtrim(B1_DE' +
        'SC) as Produto, H6_MOTIVO as Motivo, Descricao, Grupo, Regra'#9'   '
      #9#9'from P12OFICIAL.dbo.SH6010 H6 (nolock)'
      
        #9#9'Inner Join P12OFICIAL.dbo.SB1010 B1 (nolock) ON B1_FILIAL = '#39'0' +
        '101'#39
      #9#9#9#9#9#9#9#9#9#9#9#9#9'AND B1_COD = H6_PRODUTO '
      #9#9#9#9#9#9#9#9#9#9#9#9#9'AND B1.D_E_L_E_T_ <> '#39'*'#39
      
        #9#9'Left Join BomixBI.dbo.Pcp_TB_OrdemProducaoHorasImprodutivasTip' +
        'o Tipo (nolock) ON Tipo.Empresa = '#39'0101'#39
      #9#9#9#9#9#9#9#9#9#9#9#9#9#9#9#9#9#9#9#9#9'  AND Tipo.Codigo = H6_MOTIVO'
      #9#9'Where H6_FILIAL = '#39'010101'#39
      #9#9'AND H6.D_E_L_E_T_ <> '#39'*'#39
      #9#9'AND H6_FSSETOR in ('#39'INJECAO SM'#39','#39'PERSONALIZACAO'#39')'
      
        #9#9'AND Convert(Datetime,H6_FSDTTU,112) = '#39'2019-09-25 00:00:00.000' +
        #39
      ') TB'
      'Order by DataTurno, Recurso, HoraInicial')
