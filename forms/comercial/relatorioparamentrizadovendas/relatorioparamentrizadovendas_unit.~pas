procedure TRelatorioParametrizadoVendas.ppVendedorXItemStartPage(
  Sender: TObject);
begin
  ppLBL_DataVendedorXItem.Caption := 'Data Impresso: ' + DateTimeToStr(Date());
  ppLBL_NumeroPaginaVendedorXItem.Caption := ('Pgina ' + IntToStr(ppVendedorXItem.Page) + ' de ' + IntToStr(ppVendedorXItem.PageCount));

  if CBX_TipoPedido.Text = 'PARAMETRIZADO DE PEDIDOS DE VENDA' then
       ppLabel112.Caption := 'Relatrio Parametrizado de Pedidos (Vendedor x Item)'
  else
       ppLabel112.Caption := 'Relatrio Parametrizado de Vendas (Vendedor x Item)';


end;



