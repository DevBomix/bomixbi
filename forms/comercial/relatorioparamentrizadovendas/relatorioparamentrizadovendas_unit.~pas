  Sender: TObject);
begin
  ppLBL_DataVendedorXNota.Caption := 'Data Impresso: ' + DateTimeToStr(Date());
  ppLBL_NumeroPaginaVendedorXNota.Caption := ('Pgina ' + IntToStr(ppVendedorXNota.Page) + ' de ' + IntToStr(ppVendedorXNota.PageCount));

